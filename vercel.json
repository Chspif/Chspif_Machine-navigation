{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, s-maxage=31536000, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*\.(js|css|html|json))",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, s-maxage=86400, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*\.(jpeg|jpg|png|gif|ico|svg|woff|woff2|ttf|eot))",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}